<p id="notice"><%= notice %></p>

<h1>Video Request Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @video_request_comments.each do |video_request_comment| %>
      <tr>
        <td><%= link_to 'Show', video_request_comment %></td>
        <td><%= link_to 'Edit', edit_video_request_comment_path(video_request_comment) %></td>
        <td><%= link_to 'Destroy', video_request_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video Request Comment', new_video_request_comment_path %>
